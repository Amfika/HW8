<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Smoke tests">
    <test name="First tests" parallel="true" thread-count="4">
            <classes>
                <class name="AnimalsTest">
                    <methods>
                        <include name="howMany"/>
                        <include name="howManyLionBreathe"/>
                        <include name="howManyLionMove"/>
                    </methods>
                </class>
                <class name="BirdsTest">
                    <methods>
                        <include name="howManyDoveBreathe"/>
                        <include name="howManyEagleBreathe"/>
                    </methods>
                </class>
        </classes>
    </test>
</suite>